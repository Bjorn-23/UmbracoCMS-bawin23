@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@inject Umbraco.Cms.Web.Common.UmbracoHelper UmbracoHelper
@{
    var rootContents = UmbracoHelper.ContentAtRoot();
    var settings = rootContents?.FirstOrDefault(x => x.ContentType.Alias == "settingsPage") ?? null;
    var officeLocation = settings?.Value<string>("officeLocation") ?? "";
    var socialMediaList = settings?.Value<IEnumerable<string>>("socialMediaLinks") ?? null;
}

<div id="topbar" class="bg-primary">
    <div class="container">
        <div class="address">@officeLocation</div>
    </div>
    <div class="social-media">
        @if (socialMediaList != null)
        @foreach (var socialMedia in @socialMediaList)
        {
            <a href=$"https://www.{@socialMedia}.com" target="_blank" class="@socialMedia"><i class="fa-brands fa-@socialMedia-f"></i></a>
        }
    </div>
</div>